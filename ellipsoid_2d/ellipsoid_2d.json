{
    "Name": "Falling ellipse",
    "ShortName":"Falling ellipse",
    "Models":
    {
        "fluid":
        {
            "materials":"Fluid",
            "setup":{
                "equations":"Navier-Stokes",
                "gravity":
                {
                    "enable":1,
                    "expr":"{0,-981}"
                }
            }
        },
        "body":{
            "materials":["EllipseVolume"]
        }          
    },
    "Materials":
    {
        "Fluid":{
            "physics":"fluid",
            "rho":"1",// g/cm3
            "mu":"0.01"
        },
        "EllipseVolume":{
            "physics":"body",
            "rho":1.1
        }
    },
    "Meshes":
    {
        "fluid":
        {
            "Import":
            {
                "filename":"$cfgdir/ellipsoid_2d.geo"
            },
            "MeshMotion":
            {
                "ComputationalDomain":
                {
                    "markers":"Fluid",
                    "method":"harmonic_extension"
                },
                "Displacement":
                {
                    "Zero":["Top","Bottom","Walls"]
                }
            }
        }
    },
    "BoundaryConditions":
    {
        "fluid":
        {
            "velocity":
            {
                "Walls":
                {
                    "expr":"{0,0}"
                },
                "Bottom":
                {
                    "expr":"{0,0}"
                }
            },        
            "outlet":
            {
                "Top":
                {
                    "expr":"{0,0}" 
                }
            },
            "body":
            {
                "EllipseSurface":
                {
                    "markers":["EllipseSurface"],
                    "materials":
                    {
                        "names":["EllipseVolume"]
                    }
                }
            }
        }
    },
    "PostProcess":
    {
        "fluid":{
            "Exports":
            {
                "fields":["velocity","pressure","pid","displacement"]
            },
            "Measures":
            {
                "Quantities":
                {
                    "names":"all"
                }
            }
        }
    }

}

