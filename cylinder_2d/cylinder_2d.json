{
    "Name": "Falling cylinder confined",
    "ShortName":"Falling cylinder confined",
    "Models":
    {
        "fluid":
        {
            "materials":"Fluid",
            "setup":{
                "equations":"Navier-Stokes",
                "gravity":
                {
                    "enable":1,
                    "expr":"{0,-980}"
                }
            }
        },
        "body":{
            "materials":["MySolid"]
        }        
    },
    "Materials":
    {
        "Fluid":{
            "markers":"Fluid",
            "physics":"fluid",
            "rho":"1",
            "mu":"0.1" 
        },
        "MySolid":{
            "markers":"CylinderVolume",
            "physics":"body",
            "rho":"1.25"
        }
    },
    "Parameters":
    {

    },
    "Meshes":
    {
        "fluid":
        {
            "Import":
            {
                "filename":"$cfgdir/cylinder_2d.geo"
            },
            "MeshMotion":
            {
                "ComputationalDomain":
                {
                    "markers":"Fluid",
                    "method":"harmonic_extension"
                },
                "Displacement":
                {
                    "Zero":["Top","Bottom","Walls"]
                }
            }
        }
    },
    "BoundaryConditions":
    {
        "fluid":
        {
            "velocity":
            {
                "Walls":
                {
                    "expr":"{0,0}"
                },
                "Bottom":
                {
                    "expr":"{0,0}"
                }
            },        
            "outlet":
            {
                "Top":
                {
                    "expr":"{0,0}" 
                }
            },
            "body":
            {
                "p1_mark":
                {
                    "markers":"CylinderSurface",
                    "materials":{
                        "names":"MySolid"
                    }
                }
            }
        }
    },
    "PostProcess":
    {
        "fluid":
        {
            "Exports":
            {
                "fields":["velocity","pressure","pid","displacement"]
            },
            "Measures":
            {
                "Quantities":
                {
                    "names":"all"
                }
            }
        }
    }

}

