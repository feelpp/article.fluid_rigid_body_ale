{
    "Name": "Oscillating falling ellipsoid chi=1.5",
    "ShortName":"Ellipsoid chi=1.5",
    "Models":
    {
        "fluid":
        {
            "materials":"Fluid",
            "setup":{
                "equations":"Navier-Stokes",
                "gravity":
                {
                    "enable":1,
                    "expr":"{0,0,Ug*Ug*d*d/((rho_tilde-1)*Vp)}:t:Ug:d:rho_tilde:Vp"
                }
            }
        },
        "body":{
            "materials":["ParticleVolume"]
        }
    },
    "Meshes":
    {
        "fluid":
        {
            "Import":
            {
                "filename":"$cfgdir/C1-tilted.geo"
            },
            "MeshMotion":
            {
                "ComputationalDomain":
                {
                    "markers":"Fluid",
                    "method":"harmonic_extension"
                },
                "Displacement":
                {
                    "Zero":["Top","Bottom","SideCylinder"]
                }
            }
        }
    },
    "Materials":
    {
        "Fluid":{
            "rho":"1",
            "mu":"0.1"
        },
        "ParticleVolume":{
            "rho":"2.14" // rho_p/rho_f = 2.1
        }
    },
    "Parameters":
    {
        "chi":1.5,
        "d":1,
        "Ga":150,
        "Vp":"3.1415*d*d*d/(6*chi):d:chi",
        "Ug":"Ga*materials_Fluid_mu/(materials_Fluid_rho*d):Ga:d:materials_Fluid_mu:materials_Fluid_rho",
        "rho_tilde":"materials_ParticleVolume_rho/materials_Fluid_rho:materials_Fluid_rho:materials_ParticleVolume_rho"
    },
    "BoundaryConditions":
    {
        "fluid":
        {
            "velocity":
            {
                "Bottom":
                {
                    "expr":"{0,0,0}"
                }
            },
            "normal_stress":
            {
                "Top":{"expr":0},
                "SideCylinder":
                {
                    "expr":0
                }
            },
            "body":
            {
                "ParticleSurf":
                {
                    "markers":["ParticleSurf"],
                    //"materials":"MySolid",
                    "materials":
                    {
                        "names":["ParticleVolume"]
                    }
                }
            }
        }
    },
    "PostProcess":
    {
        "fluid":
        {
            "Exports":
            {
                "fields":["velocity","pressure","pid","displacement"]/*,
                "expr":{
                    "U":"{fluid_body_ParticleSurf_V_0,fluid_body_ParticleSurf_V_1,flufluid_body_ParticleSurf_V_2}:fluid_body_ParticleSurf_V_0:fluid_body_ParticleSurf_V_1:fluid_body_ParticleSurf_V_2"
                    /*"ur":"[fluid_Ux-fluid_body_CircleCenter_V_0,fluid_Uy-fluid_body_CircleCenter_V_1,fluid_Uz-fluid_body_CircleCenter_V_2]:fluid_Ux:fluid_Uy:fluid_Uz:fluid_body_CircleCenter_V_0,fluid_body_CircleCenter_V_1,fluid_body_CircleCenter_V_2",
                    "ur_parall":"-ur/Ug:ur:Ug",
                    "Re_parall":"sqrt(fluid_body_CircleCenter_V_0^2+fluid_body_CircleCenter_V_1^2+fluid_body_CircleCenter_V_2^2)*Ga/Ug:Ug:Ga:fluid_body_CircleCenter_V_0:fluid_body_CircleCenter_V_1:fluid_body_CircleCenter_V_2",
                    "uPV":"fluid_body_CircleCenter_V_2/Ug:fluid_body_CircleCenter_V_2:Ug",
                    "uPH":"{,,0}"
                }*/
            },
            "Measures":
            {                
                "Quantities":
                {
                    "names":"all"
                }
            }
        }
    }

}

