{
    "Name": "Oscillating falling ellipsoid chi=1.5",
    "ShortName":"Ellipsoid chi=1.5",
    "Models":
    {
        "fluid":
        {
            "materials":"Fluid",
            "setup":{
                "equations":"Navier-Stokes",
                "gravity":
                {
                    "enable":1,
                    "expr":"{0,0,-g}:g"//Ug*Ug*d*d/((rho_tilde-1)*Vp)}:t:Ug:d:rho_tilde:Vp"
                }
            }
        },
        "body":{
            "materials":["ParticleVolume"]
        }
    },
    "Meshes":
    {
        "fluid":
        {
            "Import":
            {
                "filename":"$cfgdir/C1-tilted.geo"
            },
            "MeshMotion":
            {
                "ComputationalDomain":
                {
                    "markers":"Fluid",
                    "method":"harmonic_extension"
                },
                "Displacement":
                {
                    "Zero":["Top","Bottom","SideCylinder"]
                }
            }
        }
    },
    "Materials":
    {
        "Fluid":{
            "rho":"1",
            "mu":"Ug*d*materials_Fluid_rho/Ga:Ga:materials_Fluid_rho:d:Ug"
        },
        "ParticleVolume":{
            "rho":"materials_Fluid_rho*rho_tilde:materials_Fluid_rho:rho_tilde" // rho_p/rho_f = 2.1
        }
    },
    "Parameters":
    {
        "chi":1.5,
        "d":1,
        "Ga":150,
        "Ug":1,//"sqrt((rho_tilde-1)*g*Vp/(d*d)):d:Vp:g:rho_tilde",
        "g":"Ug*Ug*d*d/(Vp*(rho_tilde-1)):Ug:d:Vp:rho_tilde",//100,        
        "Vp":"3.1415*d*d*d/(6*chi):d:chi",        
        "rho_tilde":2.14//"materials_ParticleVolume_rho/materials_Fluid_rho:materials_Fluid_rho:materials_ParticleVolume_rho"
    },
    "BoundaryConditions":
    {
        "fluid":
        {
            "velocity":
            {
                "Bottom":
                {
                    "expr":"{0,0,0}"
                }
            },
            "normal_stress":
            {
                "Top":{"expr":0},
                "SideCylinder":
                {
                    "expr":0
                }
            },
            "body":
            {
                "ParticleSurf":
                {
                    "markers":["ParticleSurf"],
                    //"materials":"MySolid",
                    "materials":
                    {
                        "names":["ParticleVolume"]
                    }                    
                }
            }
        }
    },
    "PostProcess":
    {
        "fluid":
        {
            "Exports":
            {
                "fields":["velocity","pressure","pid","displacement"]
            },
            "Measures":
            {                
                "Quantities":
                {
                    "names":"all"
                }
            }
        }
    }

}

